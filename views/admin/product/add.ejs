<html>
    <head>
        <title></title>
    </head>
    <body>
        <form action="/admin/product/create" method="POST" enctype="multipart/form-data">
            <label>Name</label>
            <input type="text" name="name">
            <br>
            <br>
            <label>Price</label>
            <input type="text" name="price">
            <br>
            <br>
            <label>Stock</label>
            <input type="text" name="stock">
            <br>
            <br>
            <label>Image</label>
            <input type="file" name="image">
            <br>
            <br>
            <label>Category</label>
            <select name="ProductCategoryId" id="">
                <% for(let i in result){ %>
                    <option value="<%= result[i].id %>"><%= result[i].category_name %></option>
                <% } %>
            </select>
            <br>
            <br>
            <input type="submit" value="add">
        </form>
    </body>
</html>